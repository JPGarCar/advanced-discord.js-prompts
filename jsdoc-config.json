{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "exclude": ["./node_modules/", "./spec/", "./docs/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs",
        "recursive": true,
        "access": "all",
        "package": "./package.json",
        "readme": "./README.md",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "title": "Advanced Discord.js Prompts",
            "search": {
                "shouldSort": true,
                "threshold": 0.4,
                "location": 0,
                "distance": 100,
                "maxPatternLength": 32,
                "minMatchCharLength": 1
            },
            "menu": [
                {
                    "title": "GitHub",
                    "link": "https://github.com/JPGarCar/advanced-discord.js-prompts"
                },
                {
                    "title": "NPM",
                    "link": "https://www.npmjs.com/package/advanced-discord.js-prompts"
                }
            ]
        }
    }
}