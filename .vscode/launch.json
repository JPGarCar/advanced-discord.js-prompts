    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Jasmine Tests",
            "program": "${workspaceRoot}\\node_modules\\jasmine\\bin\\jasmine.js",
            "env": {
                "NODE_PATH": "."
            }
        }
    ]
}